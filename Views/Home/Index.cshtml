        <!-- ApexCharts Scripts -->
        <script src="https://cdn.jsdelivr.net/npm/apexcharts"></script>

        <script>
            document.addEventListener("DOMContentLoaded", () => {

                // Pie Chart - Difficulty Level
                var difficultyOptions = {
                    series: [30, 25, 20, 15, 10],
                    chart: {
                        type: 'pie',
                        height: 350,
                        toolbar: {
                            show: true,  // ENABLE TOOLBAR
                            tools: {
                                download: true,
                                selection: false,
                                zoom: false,
                                zoomin: false,
                                zoomout: false,
                                pan: false,
                                reset: false
                            }
                        }
                    },
                    labels: ['Easy', 'Medium', 'Hard', 'Very Hard', 'Expert'],
                    colors: ['#28a745', '#ffc107', '#fd7e14', '#dc3545', '#6f42c1'],
                    responsive: [{
                        breakpoint: 480,
                        options: {
                            chart: { width: 300 },
                            legend: { position: 'bottom' }
                        }
                    }]
                };
                new ApexCharts(document.querySelector("#difficultyChart"), difficultyOptions).render();

                // Bar Chart - Top 5 Quizzes (Actual Data)
                var participationOptions = {
                    series: [{
                        name: 'Number of Questions',
                        data: [30, 25, 20, 15, 13]
                    }],
                    chart: {
                        type: 'bar',
                        height: 350,
                        toolbar: {
                            show: true,  // ENABLE TOOLBAR
                            tools: {
                                download: true,
                                selection: false,
                                zoom: false,
                                zoomin: false,
                                zoomout: false,
                                pan: false,
                                reset: false
                            }
                        }
                    },
                    colors: ['#4154f1'],
                    plotOptions: {
                        bar: { borderRadius: 8, horizontal: false, columnWidth: '45%' }
                    },
                    dataLabels: { enabled: true },
                    xaxis: {
                        categories: ['Literature Quiz', 'Geography Quiz', 'History Quiz', 'Science Quiz', 'Math Quiz'],
                        labels: { rotate: -20 }
                    },
                    yaxis: {
                        title: { text: 'Questions' }
                    },
                    grid: { borderColor: '#f1f1f1' }
                };
                new ApexCharts(document.querySelector("#participationChart"), participationOptions).render();

                            
            });
        </script>

